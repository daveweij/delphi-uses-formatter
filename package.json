{
    "name": "pascal-uses-formatter",
    "displayName": "pascal-uses-formatter",
    "description": "sorts the uses section in pascal files",
    "version": "0.0.1",
    "publisher": "tuncb",
    "repository": "https://github.com/tuncb/delphi-uses-formatter",
    "license": "Apache-2.0",
    "engines": {
        "vscode": "^1.34.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:pascal"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.formatUses",
                "title": "Format uses section"
            }
        ],
        "languages": [
            {
                "id": "pascal",
                "extensions": [
                    ".pas"
                ]
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTests.js",
        "deploy": "vsce publish -p"
    },
    "devDependencies": {
        "@types/chai": "^4.1.7",
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.6",
        "@types/node": "^8.10.25",
        "@types/vscode": "^1.34.0",
        "chai": "^4.2.0",
        "glob": "^7.1.4",
        "mocha": "^6.1.4",
        "source-map-support": "^0.5.12",
        "tslint": "^5.16.0",
        "typescript": "^3.5.1",
        "vsce": "^1.69.0",
        "vscode-test": "^1.0.0"
    }
}
